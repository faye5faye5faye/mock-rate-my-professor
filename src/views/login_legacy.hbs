<h1>Please enter your username and password below:</h1>

<form action="/api/users/login" method="POST">
    <a href="/signup">sign up</a>
    <div>
        <label>Username:</label>
        <input id="a" type="text" name="username"/>
    </div>
    <div>
        <label>Password:</label>
        <input id="b" type="password" name="password"/>
    </div>
    <div>
        <input type="submit" value="Log In" onclick="myFunction()"/>
    </div>
</form>

<script>
function myFunction(){
    if(document.getElementById("a").value === ""){
        alert("username is required!!!!")
    }else if(document.getElementById("b").value === ""){
        alert("password is required!!!!")
    }
}
</script>

